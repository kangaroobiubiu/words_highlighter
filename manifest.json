{
  "manifest_version": 3,
  "name": "Words Highlighter",
  "version": "1.0",
  "description": "在网页中高亮显示单词、习语列表中的词或短语",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/abc.png",
      "48": "icons/abc.png",
      "128": "icons/abc.png"
    }
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_test8.js"]
    }
  ]
}
